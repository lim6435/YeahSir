<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lsg.www.mapper.MainMapper">
    <resultMap id="YsMemResult" type="YsMemVO">
        <result column="mem_id" property="memId" />
        <result column="mem_name" property="memName" />
        <result column="mem_birth" property="memBirth" />
        <result column="mem_tall" property="memTall" />
        <result column="mem_weight" property="memWeight" />
    </resultMap>

    <select id="getYsCopt" resultMap="YsMemResult" parameterType="com.lsg.www.vo.YsMemVO">
        SELECT COPT_ID,
			   COPT_NAME,
			   COPT_LOC,
			   COPT_DT,
			   COPT_TM,
			   COPT_PRID,
			   COPT_AGE_GROP,
			   COPT_MTCH_RULE,
			   COPT_FEE,
			   COPT_STAF_TEL_NO
		  FROM YS_COPT
		 

    </select>
</mapper>